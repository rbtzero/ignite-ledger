*.ipynb filter=lfs diff=lfs merge=lfs -text
*.csv   filter=lfs diff=lfs merge=lfs -text
*.bin   filter=lfs diff=lfs merge=lfs -text
*.png   filter=lfs diff=lfs merge=lfs -text
# *.pdf   removed from LFS
*.mp4   filter=lfs diff=lfs merge=lfs -text
paper/figs/*.pdf -filter -diff -merge -text
paper/figs/*.png -filter -diff -merge -text 
*.rmf filter=lfs diff=lfs merge=lfs -text
*.pi filter=lfs diff=lfs merge=lfs -text
*.fits filter=lfs diff=lfs merge=lfs -text
*.arf filter=lfs diff=lfs merge=lfs -text
*.root filter=lfs diff=lfs merge=lfs -text
